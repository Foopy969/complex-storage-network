{
  "multipart": [
    {   "apply": { "model": "complexstorage:block/cable_center" }},
    {   "when": { "OR": [ { "north": "cable" }, { "north": "inventory" } ] },
        "apply": { "model": "complexstorage:block/cable_side"}
    },
    {   "when": { "OR": [ { "east": "cable" }, { "east": "inventory" } ] },
        "apply": { "model": "complexstorage:block/cable_side", "y": 90 }
    },
    {   "when": { "OR": [ { "south": "cable" }, { "south": "inventory" } ] },
        "apply": { "model": "complexstorage:block/cable_side", "y": 180 }
    },
    {   "when": { "OR": [ { "west": "cable" }, { "west": "inventory" } ] },
        "apply": { "model": "complexstorage:block/cable_side", "y": 270 }
    },
    {   "when": { "OR": [ { "up": "cable" }, { "up": "inventory" } ] },
        "apply": { "model": "complexstorage:block/cable_side", "x": 270 }
    },
    {   "when": { "OR": [ { "down": "cable" }, { "down": "inventory" } ] },
        "apply": { "model": "complexstorage:block/cable_side", "x": 90 }
    }
  ]
}
